{
  "name": "frontend",
  "private": true,
  "type": "module",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "build": "react-router build",
    "build:storybook": "storybook build",
    "dev": "concurrently \"react-router dev\" \"pnpm run storybook\"",
    "start": "docker compose up",
    "start:react-router": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint . --max-warnings 200 && tsc --noEmit",
    "lint:eslint": "eslint . --max-warnings 200",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test": "pnpm run test:vitest && concurrently -k -s first -n \"SB,TEST\" -c \"bgBlue.bold,bgMagenta.bold\" \"pnpm run build-storybook && pnpx http-server storybook-static --port 6006 --silent\" \"pnpx wait-on tcp:6006 && pnpm run test-storybook --maxWorkers=2 --testTimeout=60000\"",
    "test:watch": "vitest",
    "test:vitest": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test-storybook": "test-storybook --verbose",
    "debug:endpoints": "node debug-edit-endpoints.js",
    "debug:edit": "node debug-edit.mjs"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^12.1.0",
